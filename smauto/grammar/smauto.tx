import automation
import broker
import entity

SmartAutoModel:
    'name:' name=ID
    'version:' version=STRING
    ('description:' description=STRING)?
    brokers+=Broker
    entities*=Entity
    automations*=Automation
;

// Comments
Comment: CommentLine | CommentBlock ;

CommentLine: /\/\/.*?$/;

CommentBlock: /\/\*(.|\n)*?\*\//;