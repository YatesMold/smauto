// Types

Time:
    hours=INT (':' minutes=INT)? (':' seconds=INT)?
;

// Hours: /[0-2]?[0-9]?/;
// Minutes: /[0-5][0-9]/;

List:
    '[' items*=ListItem[','] ']'
;

ListItem:
    NUMBER | STRING | BOOL | List | Dict | OBJECT
;

Dict:
    '{' items*=DictItem[','] '}'
;

DictItem:
    name=STRING ':' value=DictType
;

DictType:
    NUMBER | STRING | BOOL | Dict | List | OBJECT
;

NID: /(.*?)\n/;
